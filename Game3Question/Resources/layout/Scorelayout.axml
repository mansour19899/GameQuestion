        p1:layout_height="53.0dp"
        p1:id="@+id/txt3"
        p1:gravity="center"
        p1:textSize="20dp"
        p1:textStyle="bold"
        p1:layout_marginRight="60dp"
        p1:layout_marginLeft="60dp"
        p1:layout_marginTop="10dp"
        p1:layout_marginBottom="0dp" />
    <TextView
        p1:text="Text"
        p1:layout_width="match_parent"
        p1:layout_height="53.0dp"
        p1:id="@+id/txt4"
        p1:gravity="center"
        p1:textSize="20dp"
        p1:textStyle="bold"
        p1:layout_marginRight="60dp"
